<MailMergeLibSettings xmlns:yaxlib="http://www.sinairv.com/yaxlib/">
  <Sender>
    <MaxNumOfSmtpClients>5</MaxNumOfSmtpClients>
    <SmtpClients>
      <SmtpClientConfig Name="Standard">
        <SmtpHost>mcno-nbg2-01.internet1.de</SmtpHost>
        <SmtpPort>587</SmtpPort>
        <ClientDomain>mail.volleyball-liga.de</ClientDomain>
        <LocalEndPoint />
        <NetworkCredential Username="**enter here**" Password="**enter here**" Domain="" yaxlib:realtype="MailMergeLib.Credential" />
        <MailOutputDirectory />
        <MessageOutput>SmtpServer</MessageOutput>
        <SslProtocols>None</SslProtocols>
        <SecureSocketOptions>Auto</SecureSocketOptions>
        <Timeout>5000</Timeout>
        <DelayBetweenMessages>100</DelayBetweenMessages>
        <MaxFailures>3</MaxFailures>
        <RetryDelayTime>500</RetryDelayTime>
      </SmtpClientConfig>
    </SmtpClients>
  </Sender>
  <Message>
    <TextTransferEncoding>SevenBit</TextTransferEncoding>
    <BinaryTransferEncoding>Base64</BinaryTransferEncoding>
    <CharacterEncoding>utf-8</CharacterEncoding>
    <CultureInfo>de-DE</CultureInfo>
    <FileBaseDirectory></FileBaseDirectory>
    <IgnoreIllegalRecipientAddresses>False</IgnoreIllegalRecipientAddresses>
    <IgnoreMissingInlineAttachments>True</IgnoreMissingInlineAttachments>
    <IgnoreMissingFileAttachments>True</IgnoreMissingFileAttachments>
    <Priority>Normal</Priority>
    <StandardFromAddress></StandardFromAddress>
    <Organization>axuno gGmbH</Organization>
    <Xmailer>MailMergeLib</Xmailer>
    <SmartFormatterConfig>
      <ParseErrorAction>ThrowError</ParseErrorAction>
      <FormatErrorAction>ThrowError</FormatErrorAction>
      <CaseSensitivity>CaseSensitive</CaseSensitivity>
      <ConvertCharacterStringLiterals>True</ConvertCharacterStringLiterals>
    </SmartFormatterConfig>
  </Message>
</MailMergeLibSettings>